{
  "homepage": "/markdown-doc-api",
  "scripts": {
    "toc": "doctoc --github ./src/docs/*.md",
    "babel-script": "rm -rf ./scripts/__lib ./public/docs && babel ./scripts --out-dir ./scripts/__lib --copy-files --no-copy-ignored",
    "start": "yarn dir-start && webpack serve --config ./scripts/__lib/start.js",
    "build": "yarn dir-start && webpack --mode=production --config ./scripts/__lib/build.js && node ./scripts/__lib/dir/copyfile.js",
    "dir-start": "yarn babel-script && node ./scripts/__lib/dir/fs.js",
    "build:ghpages": "env-cmd -f .env.ghpages npm run build",
    "predeploy": "npm run build:ghpages",
    "deploy": "gh-pages -b gh-pages -d build",
    "serve": "serve -s build -l 4000"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.0",
    "doctoc": "^1.4.0",
    "env-cmd": "^10.1.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.0.1",
    "gh-pages": "^4.0.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "markdown-loader": "^6.0.0",
    "marked": "^1.2.3",
    "md5": "^2.3.0",
    "mini-css-extract-plugin": "^1.3.3",
    "sass": "^1.28.0",
    "sass-loader": "^10.0.5",
    "string.prototype.matchall": "^4.0.2",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0"
  }
}
